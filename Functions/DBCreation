CREATE TABLE users
(
    userID INTEGER PRIMARY KEY,
    username TEXT,
    passwordHash TEXT,
    latestSession TEXT,
    expiry DATETIME
);

CREATE TABLE devices
(
    deviceID INTEGER PRIMARY KEY,
    DeviceName TEXT,
    InternalIP TEXT,
    ExternalIP TEXT,
    PortConnection TEXT
);

